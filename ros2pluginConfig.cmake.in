# CMake package configuration file for the plugin

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(PROJECT ros2plugin)

find_package(SofaCore REQUIRED)
find_package(ros2_types REQUIRED)
find_package(Qt5 COMPONENTS Core Widgets Network REQUIRED)
find_package(OpenCV REQUIRED)
find_package(Threads REQUIRED)
find_package(Eigen3 REQUIRED )
find_package(image REQUIRED)
find_package(SofaGuiQt REQUIRED)
find_package(SofaScheduler REQUIRED)
find_package(image_transport REQUIRED)
find_package(trajectory_msgs REQUIRED)

if(NOT TARGET ${PROJECT})
        include("${CMAKE_CURRENT_LIST_DIR}/${PROJECT}Targets.cmake")
endif()

check_required_components(${PROJECT})





